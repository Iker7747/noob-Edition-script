-- loadstring
local ESP = loadstring(game:HttpGet("https://kiriot22.com/releases/ESP.lua"))()
 
-- config
ESP.Players = true
ESP.Boxes = true
ESP.Names = true
ESP:Toggle(true)
 
-- object
ESP:AddObjectListener(Workspace.RakoofNPC.Torso, { -- Object Path, For example: Workspace.ThisFolder
    Name = "Bone", --Object name inside of the path, for example: Workspace.ThisFolder.Item_1
    CustomName = "Rakoof", -- Name you want to be displayed
    Color = Color3.fromRGB(255, 0, 0), -- Color
    IsEnabled = "Rakoof" -- Any name, has to be the same as the last line: ESP.TheNameYouWant
})
ESP.Rakoof = true
